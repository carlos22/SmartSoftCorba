##########################
# server port parameter

[eventServer]
serviceName eventServer

[paramServer]
serviceName param

[posePushTimedServer]
serviceName currentPose
cycle 0.1

[requestScan3dSendServer]
serviceName request3dscan

[scan3dEventServer]
serviceName newScan

[scan3dQueryServer]
serviceName 3dscan

[stateServer]
serviceName stateServer

[trajectorySendServer]
serviceName performTrajectory

##########################
# client port parameter

[basePushTimedClient]
serviceName basestate
serverName smartPioneerBaseServer
interval 1

[laserQueryClient]
serviceName scan
serverName SmartLaserHokuyoURGServer

[laserStateClient]
serviceName stateServer
serverName SmartLaserHokuyoURGServer

##########################
# other parameter

[base]
base_a 0
on_base true
steer_a 0
turret_a 0
x 0
y 0
z 0

[katana]
azimuth 0
elevation 0
fast_calibration false
goal_angle_approach 0.1
goal_position_approach 3
katana_config_file src/config/katana6M180.cfg
katana_device Serial
roll 0
verbose false
x 0
y 0
z 0

[laser]
azimuth 0
elevation 0
high_precision true
laser_is_mounted false
roll 0
x 0
y 0
z 0

[network]
ip_adress 127.0.0.1
port 5566

[serial_port]
device /dev/ttyS0

